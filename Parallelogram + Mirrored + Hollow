#include<stdio.h>
 int main()
 {
     int i,j,s,t;
     printf("Enter the numbers");
     scanf("%d %d",&s,&t);
     for(i=0;i<s;i++)
     {
         for(j=s;j>i;j--)
         {
             printf(" ");
         }
         for(j=0;j<t;j++)
         {
             if(i==0 || i==s-1 || j==0 || j==t-1)
             {
             printf("*");
         }
         else 
         {
             printf(" ");
         }
         }
         printf("\n");
     }
 }
