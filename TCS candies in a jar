#include<stdio.h>
int main()
{
int N=10,k=5;
int num;
printf("Enter the number: ");
scanf("%d",&num);
if(num>=1 &&  num<=5)
{
    printf("Number of candies sold:%d \n",num);
    printf("Number of candies available:%d \n",N-num);
    
}
else 
{
    printf("Invalid input \n");
    printf("Number of candies available: %d \n",N);
    
}
}
